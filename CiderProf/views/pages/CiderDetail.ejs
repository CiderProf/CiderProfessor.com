<section>
    <img src="<%=cider.PicURL%>"> 
    <ul>
        <li><h2> <%=cider.Name%> </h2></li>
        <li> <%=cider.Cidery%> </li>
        <li>Grade: <%=cider.Grade%> </li>
    </ul>
    <ul>
        <% Object.keys(cider.Style).forEach(s => { %>
            <li>Style: <%=s%>
            <% if(cider.Style[s] != null){ %>
                - <%=cider.Style[s]%>
            <% }; %>
            </li>
        <% }); %>
        <li>Mood: <%=cider.Mood%> </li>
        <li>Location: <%=cider.State_Country%> </li>
        <li>Date Tried:  </li>
        <li>ABV: <%=cider.ABV%>%</li>
    </ul>
</section>
<section>
    <h3>My Feedback: </h3>
    <p><%=cider.Feedback%></p>
</section>
<section>
    <div> 
        <h2>Product Description:</h2>
        <p><%=cider.Prodct_Description%> </p>
    </div>
    <img src="<%=cider.LogoURL%>">
</section>
<section> <!---If no comparative lit, hide-->
<!---Link to comp lit rev with title of Rev-->
</section>


